<div class="cart_title" ><% t('.title')%></div>
<table>
   <%= render(cart.line_items) %>
   <tr class="total_line" >
       <td colspan="2" >Total</td>
       <td class="total_cell" ><%= number_to_currency(cart.total_price) %></td>
   </tr>
</table>


<%= link_to image_tag("/images/paypal_express_Checkout.jpg"),:controller => "orders" ,:action => "checkout" %>
<%= button_to t('.checkout'), new_order_path(cart), :method => :get,:id => 'chkout'%>
<%= button_to t('.empty'), cart, :method => :delete,
:confirm => 'Are you sure?' %>
